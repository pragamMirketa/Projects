<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Recurring_False_Frequency</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT( Recurring__c ), NOT( ISPICKVAL( Frequency__c , &quot;&quot;) ) )</errorConditionFormula>
    <errorDisplayField>Frequency__c</errorDisplayField>
    <errorMessage>User cannot select Frequency field</errorMessage>
</ValidationRule>
